<body text="#000000">
<form id="Form1" runat="server">
        <div align="left">
            <!-- START HEADER HERE -->
            <table width="100%" cellspacing="0" cellpadding="0" border="0">
                <tbody>
                    <tr>
                        <td bgcolor="#0b86c3">
                            <img src="Styles/images/banner.jpg" alt="" border="0" style="width: 938px" />
                        </td>
                        <td width="100%" bgcolor="#0b86c3" align="right">
                            <div align="right">
                                <img src="../Styles/images/right-side.jpg"></img>
                            </div>
                        </td>
                    </tr>
                </tbody>
            </table>
            <div id="tray" class="box">
                <p class="f-left box">
                </p>
            </div>
            <!-- END HEADER HERE -->
            <!-- PAGE CONTENT -->
            <table border="0" cellpadding="3" cellspacing="0" width="100%">
                <tbody>
                    <tr>
                        <td background="../Styles/images/bgr.gif" valign="top" width="140">
                            <!-- START SIDEBAR -->

                            <div class="Menu_Items"> 
                             <asp:Menu ID="menuBar" runat="server" Orientation="Vertical" Width="100%" MaximumDynamicDisplayLevels="10">
                                <StaticMenuStyle CssClass="sf-menu sf-vertical" />
                            </asp:Menu>
                        </div>
                        <br />
                        <!-- END LOGIN FORM -->
                    </td>
                    <td valign="top" width="100%">
                        <img alt="" src="../Styles/images/spacer.gif" height="1" border="0" width="100%" />
                        <div id="content" class="box">
                            <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server" />
                        </div>
                    </td>
                    <td valign="top">
                        <img alt="" src="../Styles/images/spacer.gif" height="1" border="0" width="1">
                    </td>
                </tr>
            </tbody>
        </table>
        <!-- ENDING OF PAGE BODY -->
        </div>
    <!-- footer banner start -->
        <div id="footer-banner">
        </div>
        <!-- footer banner end -->
        <!-- credit start -->
        <div id="credit">
            <div class="wrap">
                <br />
                <!-- design by johanksi -->
                <div class="clear">
                </div>
            </div>
        </div> 
        <!-- credit start -->
    </form>
</body>
</html>

