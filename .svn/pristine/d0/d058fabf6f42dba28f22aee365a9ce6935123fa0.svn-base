	font-weight: Bold;
	cursor: hand;
	width: 100%;
	font-size:8pt;
	text-transform: uppercase 
}


.Menu_Head_KPI {
	filter:alpha(opacity=100);
	background-color: #cc0000;
	color: #000000;
	font-weight: Bold;
	cursor: hand;
	width: 100%;
	font-size:8pt;
	text-transform: uppercase 
}
.Menu_Items_KPI {
	filter:alpha(opacity=100);
	background-color: #ffffff;
	border-width: thin;
	border-color: #cc0000;
	border-style: solid;
