
@{ ViewData["Title"] = "Lihat Detail Proposal Penelitian "; }

<!DOCTYPE html>


<html>
<head>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
    <link rel="stylesheet" type="text/css" href="~/css/mapStyle.css" />
    <script src="~/js/mapJavaScript.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/@@mdi/font@5.x/css/materialdesignicons.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/font-awesome-4.7.0/css/font-awesome.min.css">
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/style2.css" />
    <!-- Date Picker -->
    <script type="text/javascript" src="//cdn.jsdelivr.net/bootstrap.daterangepicker/2/daterangepicker.js"></script>
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/bootstrap.daterangepicker/2/daterangepicker.css" />
    <link rel="stylesheet" type="text/css" href="~/css/bootstrap-datepicker.min.css" />
    <script type="text/javascript" src="~/js/bootstrap-datepicker.min.js"></script>
    <!--Mapbox-->
    <link href="https://api.mapbox.com/mapbox-gl-js/v2.4.1/mapbox-gl.css"
          rel="stylesheet" />
    <script src="https://api.mapbox.com/mapbox-gl-js/v2.4.1/mapbox-gl.js"></script>
    <script src="https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-geocoder/v4.7.2/mapbox-gl-geocoder.min.js"></script>
    <link rel="stylesheet"
          href="https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-geocoder/v4.7.2/mapbox-gl-geocoder.css"
          type="text/css" />
    <script src="https://unpkg.com/@@turf/turf@6/turf.min.js"></script>
    <!-- Data Table-->
    @*<link rel="stylesheet" href="https://cdn.datatables.net/1.10.25/css/dataTables.bootstrap4.min.css">*@
    <style>
        #anggota {
            max-height: 300px;
            overflow-y: auto;
        }

        .button {
            border: none;
            color: white;
            padding: 7px 14px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            font-size: 16px;
            margin: 4px 2px;
            transition-duration: 0.4s;
            cursor: pointer;
            margin-top: 20px;
            margin-left: 90px;
            border-radius: 20px;
        }

        .style1 {
            background-color: #fafafa;
            border: 0;
            box-shadow: 0 0 4px rgba(0,0,0,0.3);
            transition: .3s box-shadow;
            margin-bottom: 10px;
            height: 30px;
            width: 450px;
        }

            .style1:hover {
                box-shadow: 0 0 4px rgba(0,0,0,0.5);
            }

        .button1 {
            background-color: white;
            color: black;
            border: 2px solid #261ad9;
        }

            .button1:hover {
                background-color: #3b4efc;
                color: white;
            }

        .button2 {
            background-color: #129e42;
            color: black;
            border: 2px solid #129e42;
        }

            .button2:hover {
                background-color: #13843a;
                color: white;
            }

        .button3 {
            background-color: #2535c9;
            color: black;
            border: 2px solid #2535c9;
        }

            .button3:hover {
                background-color: #1a2695;
                color: white;
            }

        .button4 {
            background-color: #F2AA4C;
            color: black;
            border: 2px solid #F2AA4C;
            padding: 3px 7px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            font-size: 16px;
            margin: 4px 2px;
            transition-duration: 0.4s;
            cursor: pointer;
            border-radius: 15px;
        }

        body {
            font-family: 'Karla', sans-serif;
        }

        .content-wrapper {
            border-radius: 15px;
            box-shadow: 0 5px 8px 0 rgba(0,0,0,0.2);
            transition: 0.3s;
            border-radius: 10px;
            min-height: 190px !important;
            background-color: transparent;
        }

        .radio-checkbox {
            font-size: large;
            letter-spacing: 1px;
        }

        .title-card {
            color: #FFFFFF;
            background-color: #F2AA4C;
            padding: 15px;
            border-radius: 10px 10px 0px 0px;
            min-width: inherit;
        }

        .blue-color {
            background-color: #1d84C6 !important;
        }

        .green-color {
            background-color: #1CB394 !important;
        }

        .btn:active {
            background-color: #ffe7ab;
            transform: translateY(4px);
        }

        .btn:hover {
            background-color: #ffe7ab;
        }

        .td, th {
            vertical-align: middle;
        }

        table {
            border-collapse: collapse;
            width: 100%;
        }

        th, td {
            padding: 8px;
            text-align: left;
            border-top: 1px solid #ddd;
            border-bottom: 1px ridge #ddd;
        }

            th, td .publikasi {
                padding: 8px;
                text-align: left;
                width: 30px;
                border-top: 1px solid #ddd;
                border-bottom: 1px ridge #ddd;
            }

            td .style8 {
                font-weight: bold;
            }
    </style>
</head>
<body>

    <div class="px-3 mt-3">
        <div class="content-wrapper p-3">
            <span style=" background-color:aliceblue; font-weight:bolder; margin-bottom: 20px;"><i class="fas fa-edit" style="margin-right: 20px;"></i> DETAIL PROPOSAL PENELITIAN</span>
            <hr />

            <nav>
                <div class="nav nav-tabs" id="nav-tab" role="tablist">
                    <a class="nav-item nav-link active" id="nav-data-tab" data-toggle="tab" href="#nav-data" role="tab" aria-controls="nav-data" aria-selected="true">Data Penelitian</a>
                    <a class="nav-item nav-link" id="nav-identitas-tab" data-toggle="tab" href="#nav-identitas" role="tab" aria-controls="nav-identitas" aria-selected="false">Identitas Peneliti</a>
                    <a class="nav-item nav-link" id="nav-cv-tab" data-toggle="tab" href="#nav-cv" role="tab" aria-controls="nav-cv" aria-selected="false">CV-Pengalaman Penelitian (5 Tahun terakhir)</a>
                    <a class="nav-item nav-link" id="nav-publikasi-tab" data-toggle="tab" href="#nav-publikasi" role="tab" aria-controls="nav-publikasi" aria-selected="false">Publikasi</a>

                </div>
            </nav>
            @if (TempData["succ"] != null)
            {
                <div class="alert alert-success alert-dismissible fade show" role="alert" style="background-color:green">
                    <strong><i class='fas fa-bell' style='font-size:22px; color:floralwhite'></i>&nbsp; @TempData["succ"].ToString()</strong>
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true" style="color:white">&times;</span>
                    </button>
                </div>
            }
            @if (TempData["err"] != null)
            {
                <div class="alert alert-danger alert-dismissible fade show" role="alert">
                    <strong><i class='fas fa-exclamation-triangle' style='font-size:22px; color:floralwhite'></i>&nbsp;@TempData["err"].ToString()</strong>
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true" style="color:white">&times;</span>
                    </button>
                </div>
            }
            <div class="tab-content" id="nav-tabContent">
                <div class="tab-pane fade show active" id="nav-data" role="tabpanel" aria-labelledby="nav-data-tab">
                    <div class="content-wrapper p-3" id="content" style="margin-top:10px">
                        <form method="post" asp-action="UbahPenelitian" asp-controller="Profile" enctype="multipart/form-data">

                            @Html.AntiForgeryToken()

                            <div class="row">
                                <div class="col-md-6">
                                    <label for="basic-addon1" hidden class="form-label">ID PROPOSAL</label>
                                    <input  hidden class="form-control" id="id_proposal" name="id_proposal" value="@Model.lihatPropo.id_proposal">
                                    <label for="basic-addon1" hidden class="form-label">NPP</label>
                                    <input  hidden class="form-control" id="NPP" name="NPP" value="@Model.dataPenelitian.NPP">

                                    <div class="mb-4" hidden>
                                        <div class="input-group">
                                            <input type="hidden"
                                                   autocomplete="off"
                                                   class="form-control"
                                                   name="USER_ID"
                                                   id="USER_ID"
                                                   value="@Model.cekUser.username" />
                                        </div>
                                    </div>
                                    <div class="mb-4"hidden>
                                        <div class="input-group">
                                            <input type="hidden"
                                                   autocomplete="off"
                                                   class="form-control"
                                                   name="username"
                                                   id="username"
                                                   value="@Model.cekUser.username" />
                                        </div>
                                    </div>
                                    <div class="mb-4"hidden>
                                        <div class="input-group">
                                            <input type="hidden"
                                                   autocomplete="off"
                                                   class="form-control"
                                                   name="npp"
                                                   id="npp"
                                                   value="@Model.cekUser.NPP" />
                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Skim Penelitian</label>
                                        <div class="input-group">
                                            <select style="width: 710px; height: 40px;" id="ID_SKIM" name="ID_SKIM">
                                                <option hidden style="color:lightgrey">Pilih Skim Penelitian</option>
                                                @foreach (var item in Model.refjenis1)
                                                {
                                                    <option value="@item.ID_SKIM"> @item.DESKRIPSI</option>

                                                }
                                            </select>
                                        </div>
                                    </div>

                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Tahun Akademik</label>
                                        <div class="input-group">

                                            <select style="width: 710px; height: 40px;"
                                                    id="ID_TAHUN_ANGGARAN"
                                                    name="ID_TAHUN_ANGGARAN">
                                                <option hidden style="color: lightgrey">
                                                    Pilih Tahun Akademik
                                                </option>
                                                @foreach (var item in Model.refjenis2)
                                                {
                                                    <option value="@item.ID_TAHUN_ANGGARAN">
                                                        @item.TAHUN_ANGGARAN
                                                    </option>

                                                }
                                            </select>
                                        </div>
                                    </div>

                                    <div class="mb-4">
                                        <label for="basic-addon2" class="form-label">Semester Akademik </label>
                                        <div class="input-group">
                                            <select style="width: 710px; height: 40px;" id="NO_SEMESTER" name="NO_SEMESTER">
                                                <option hidden style="color:lightgrey">Pilih Semester Akademik</option>
                                                @foreach (var item in Model.refjenis3)
                                                {
                                                    <option value="@item.NO_SEMESTER"> @item.SEMESTER_AKADEMIK</option>

                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="card alert-default">
                                        <div class="card-header">
                                            <i class="fas fa-file-export" aria-hidden="true"></i>
                                            <label for="luaranOutput"
                                                   class="col-sm-3 col-form-label col-form-label-sm">Luaran Output</label><br />
                                        </div>
                                        <div class="card-body">
                                            <div class="form-group row">

                                                <div class="col-sm-9 row" id="outputList">
                                                    <div class="col-sm-6">
                                                        @foreach (var item in Model.refjenis4)
                                                        {
                                                            <div class="col-sm-5 custom-control custom-checkbox ml-2 mr-2">
                                                                <input type="checkbox" class="custom-control-input" onclick="pushOutput()" id="output_@item.ID_OUTPUT" value="@item.DESKRIPSI">
                                                                <label class="custom-control-label font-weight-normal" for="output_@item.ID_OUTPUT">@item.DESKRIPSI</label>
                                                            </div>
                                                        }
                                                        <input type="text" class="form-control form-control-sm" name="output" id="output" readonly hidden>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card alert-default">
                                        <div class="card-header">
                                            <i class="fas fa-file-export" aria-hidden="true"></i>
                                            <label for="luaranOutcome"
                                                   class="col-sm-3 col-form-label col-form-label-sm">Luaran Outcome</label><br />
                                        </div>
                                        <div class="card-body">
                                            <div class="form-group">

                                                <div class="col-sm-9 row" id="outcomeList">
                                                    <div class="col-sm-6">
                                                        <label class="col-form-label col-form-label-sm">Publikasi</label>

                                                        @foreach (var item in Model.refOutcome)
                                                        {
                                                            if (item.ID_JENIS_OUTCOME == 1)
                                                            {
                                                                <div class="custom-control custom-checkbox">
                                                                    <input type="checkbox"
                                                                           class="custom-control-input"
                                                                           onclick="pushOutcome()"
                                                                           id="outcome_@item.ID_OUTCOME"
                                                                           value="@item.DESKRIPSI" />
                                                                    <label class="custom-control-label font-weight-normal"
                                                                           for="outcome_@item.ID_OUTCOME">@item.DESKRIPSI</label>
                                                                    </div>
                                                                }
                                                            }
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <label class="col-form-label col-form-label-sm">HaKI</label>
                                                        @foreach (var item in Model.refOutcome)
                                                        {
                                                            if (item.ID_JENIS_OUTCOME == 2)
                                                            {
                                                                <div class="custom-control custom-checkbox">
                                                                    <input type="checkbox"
                                                                           class="custom-control-input"
                                                                           onclick="pushOutcome()"
                                                                           id="outcome_@item.ID_OUTCOME"
                                                                           value="@item.DESKRIPSI" />
                                                                    <label class="custom-control-label font-weight-normal"
                                                                           for="outcome_@item.ID_OUTCOME">@item.DESKRIPSI</label>
                                                                    </div>
                                                                }
                                                            }
                                                    </div>
                                                    <input type="text"
                                                           class="form-control form-control-sm"
                                                           name="outcome"
                                                           id="outcome"
                                                           readonly
                                                           hidden />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Jarak dari Kampus UAJY</label>
                                        <div class="input-group">
                                            <span class="input-group-text" id="basic-addon1"><i class="mdi mdi-counter small" aria-hidden="true"> </i></span>
                                            <input type="text" class="form-control" value="@Model.dataPenelitian.JARAK_KAMPUS_KM" placeholder="Jarak dari Kampus UAJY" aria-label="Nomor Barcode" aria-describedby="basic-addon1" id="JARAK_KAMPUS_KM">
                                        </div>
                                    </div>
                                    <div class="card alert-default">
                                        <div class="card-header" style="border-bottom-color:lightsteelblue; border-bottom-left-radius:10px;border-bottom-right-radius:10px">
                                            <i class="fa fa-solid fa-clock" aria-hidden="true"></i>
                                            Tanggal dan Durasi Penelitian
                                        </div>

                                        <div class="card-body">
                                            <div class="form-group row">
                                                <label for="AWAL" class="col-sm-3 col-form-label col-form-label-sm ">Tanggal Mulai Penelitian</label>
                                                <div class="input-group col-sm-3">
                                                    <input class="form-control form-control-sm datepicker " type="date" id="AWAL" name="AWAL" placeholder="YYYY-MM-DD" value="@Model.dataPenelitian.AWAL" />
                                                    
                                                </div>
                                                <label for="AKHIR" class="col-sm-3 col-form-label col-form-label-sm datepicker">Tanggal Akhir Penelitian</label>
                                                <div class="col-sm-3">
                                                    <input class="form-control form-control-sm datepicker" type="date" id="AKHIR" name="AKHIR" placeholder="YYYY-MM-DD" value="@Model.dataPenelitian.AKHIR"/>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Beban SKS</label>
                                        <div class="input-group">
                                            <span class="input-group-text" id="basic-addon1"><i class="mdi mdi-counter small" aria-hidden="true"> </i></span>
                                            <input type="text" class="form-control" value="@Model.dataPenelitian.BEBAN_SKS" placeholder="Beban SKS" id="BEBAN_SKS" name="BEBAN_SKS" aria-describedby="basic-addon1">
                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Dokumen Proposal/blind Proposal</label>
                                        <div class="input-group">
                                            <span class="input-group-text" id="basic-addon1"><i class="mdi mdi-counter small" aria-hidden="true"> </i></span>
                                            <input type="FILE" class="form-control" placeholder="Dokumen Proposal" aria-describedby="basic-addon1">
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-6">
                                    <div class="mb-4">
                                        <label for="basic-addon3" class="form-label">Judul Penelitian</label>
                                        <div class="input-group">
                                            <input id="JUDUL" name="JUDUL" placeholder="Judul Penelitian" type="text" value="@Model.dataPenelitian.JUDUL" style=" height: 40px; width: 710px;">
                                        </div>
                                    </div>

                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Kata Kunci(Inggris)</label>
                                        <div class="input-group">
                                            <input type="text"
                                                   class="form-control"
                                                   id="KEYWORD"
                                                   name="KEYWORD"
                                                   placeholder="Kata Kunci"
                                                   value="@Model.dataPenelitian.KEYWORD"
                                                   aria-label="Masa Berlaku"
                                                   aria-describedby="basic-addon1" />
                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Kategori Penelitian</label>
                                        <div class="input-group">
                                            <select style="width: 710px; height: 40px;"
                                                    id="ID_KATEGORI"
                                                    name="ID_KATEGORI">
                                                <option hidden style="color: lightgrey">
                                                    Pilih Kategori Penelitian
                                                </option>
                                                @foreach (var item in Model.refjenis7)
                                                {
                                                    <option value="@item.ID_KATEGORI">
                                                        @item.DESKRIPSI
                                                    </option>

                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Tema Penelitian Universitas</label>
                                        <div class="input-group">
                                            <select style="width: 710px; height: 40px;"
                                                    id="ID_TEMA_PENELITIAN"
                                                    name="ID_TEMA_PENELITIAN">
                                                <option hidden style="color: lightgrey">
                                                    Pilih Tema Penelitian
                                                </option>
                                                @foreach (var item in Model.refjenis6)
                                                {
                                                    <option value="@item.ID_TEMA_UNIVERSITAS">
                                                        @item.DESKRIPSI
                                                    </option>

                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="card alert-default">
                                        <div class="card-header">
                                            <i class="fa fa-money-bill" aria-hidden="true"></i>
                                            Pendanaan
                                        </div>
                                        <div class="card-body">
                                            <label for="DANA_UAJY"
                                                   class="col-sm-3 col-form-label col-form-label-sm"
                                                   style="color: steelblue">Dana UAJY</label>
                                            <div class="row col-sm-12">
                                                <div class="col-sm-9 input-group input-group-sm">
                                                    <div class="input-group-prepend">
                                                        <span class="input-group-text" id="inputGroup-sizing-sm">Rp</span>
                                                    </div>
                                                    <input type="text" id="DANA_UAJY" name="DANA_UAJY" value="@Model.dataPenelitian.DANA_UAJY" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm">
                                                </div>

                                            </div>
                                            <label for="danaPribadi"
                                                   class="col-sm-3 col-form-label col-form-label-sm"
                                                   style="color: steelblue">Dana Pribadi</label>
                                            <div class="row col-sm-12">
                                                <div class="col-sm-9 input-group input-group-sm">
                                                    <div class="input-group-prepend">
                                                        <span class="input-group-text" id="inputGroup-sizing-sm">Rp</span>
                                                    </div>
                                                    <input type="text" id="DANA_PRIBADI" value="@Model.dataPenelitian.DANA_PRIBADI" name="DANA_PRIBADI" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm">
                                                </div>

                                            </div>
                                            <label for="danaEksternal"
                                                   class="col-sm-3 col-form-label col-form-label-sm"
                                                   style="color: steelblue">Dana Eksternal</label>
                                            <div class="row col-sm-12">
                                                <div class="col-sm-9 input-group input-group-sm">
                                                    <div class="input-group-prepend">
                                                        <span class="input-group-text" id="inputGroup-sizing-sm">Rp</span>
                                                    </div>
                                                    <input type="text" id="DANA_EKSTERNAL" value="@Model.dataPenelitian.DANA_EKSTERNAL" name="DANA_EKSTERNAL" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm">
                                                </div>

                                            </div>
                                            <label for="danaKerjasama"
                                                   class="col-sm-3 col-form-label col-form-label-sm"
                                                   style="color: steelblue">Dana Kerjasama</label>
                                            <div class="row col-sm-12">
                                                <div class="col-sm-9 input-group input-group-sm">
                                                    <div class="input-group-prepend">
                                                        <span class="input-group-text" id="inputGroup-sizing-sm">Rp</span>
                                                    </div>
                                                    <input type="text" id="DANA_KERJASAMA" name="DANA_KERJASAMA" value="@Model.dataPenelitian.DANA_KERJASAMA" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm">
                                                </div>
                                            </div>
                                        </div>
                                    </div>


                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Jenis Penelitian</label>
                                        <div class="input-group">
                                            <select style="width: 710px; height: 40px;"
                                                    id="ID_JENIS_PENELITIAN"
                                                    name="ID_JENIS_PENELITIAN">
                                                <option hidden style="color: lightgrey">
                                                    Pilih Jenis Penelitian
                                                </option>
                                                @foreach (var item in Model.refjenis5)
                                                {
                                                    <option value="@item.ID_JENIS_PENELITIAN">
                                                        @item.JENIS_PENELITIAN
                                                    </option>

                                                }
                                            </select>

                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Lokasi Penelitian</label>
                                        <div class="input-group">
                                            <input type="text"
                                                   class="form-control"
                                                   placeholder="Lokasi Penelitian"
                                                   aria-label="Lokasi Penelitian"
                                                   aria-describedby="basic-addon1"
                                                   value="@Model.dataPenelitian.LOKASI"
                                                   id="Lokasi" />

                                        </div>
                                    </div>
                                    <div class="card">
                                        <div class="card-body">
                                            <label class="pb-2" style="">
                                                <i class="fa fa-map" aria-hidden="true"></i>
                                                Peta Lokasi
                                                <a data-container="body"
                                                   data-toggle="popover"
                                                   data-placement="top"
