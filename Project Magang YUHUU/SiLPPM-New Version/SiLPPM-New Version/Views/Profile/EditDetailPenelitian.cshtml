@{ ViewData["Title"] = "Lihat Detail Proposal Penelitian "; }

<!DOCTYPE html>


<html>
<head>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
    <link rel="stylesheet" type="text/css" href="~/css/mapStyle.css" />
    <script src="~/js/mapJavaScript.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/@@mdi/font@5.x/css/materialdesignicons.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/font-awesome-4.7.0/css/font-awesome.min.css">
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/style2.css" />
    <!-- Data Table-->
    @*<link rel="stylesheet" href="https://cdn.datatables.net/1.10.25/css/dataTables.bootstrap4.min.css">*@
    <style>
        #anggota {
            max-height: 300px;
            overflow-y: auto;
        }

        .button {
            border: none;
            color: white;
            padding: 7px 14px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            font-size: 16px;
            margin: 4px 2px;
            transition-duration: 0.4s;
            cursor: pointer;
            margin-top: 20px;
            margin-left: 90px;
            border-radius: 20px;
        }

        .style1 {
            background-color: #fafafa;
            border: 0;
            box-shadow: 0 0 4px rgba(0,0,0,0.3);
            transition: .3s box-shadow;
            margin-bottom: 10px;
            height: 30px;
            width: 450px;
        }

            .style1:hover {
                box-shadow: 0 0 4px rgba(0,0,0,0.5);
            }

        .button1 {
            background-color: white;
            color: black;
            border: 2px solid #261ad9;
        }

            .button1:hover {
                background-color: #3b4efc;
                color: white;
            }


        .button2 {
            background-color: #129e42;
            color: black;
            border: 2px solid #129e42;
        }

            .button2:hover {
                background-color: #13843a;
                color: white;
            }

        .button3 {
            background-color: #2535c9;
            color: black;
            border: 2px solid #2535c9;
        }


            .button3:hover {
                background-color: #1a2695;
                color: white;
            }

        .button4 {
            background-color: #F2AA4C;
            color: black;
            border: 2px solid #F2AA4C;
            padding: 3px 7px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            font-size: 16px;
            margin: 4px 2px;
            transition-duration: 0.4s;
            cursor: pointer;
            border-radius: 15px;
        }

        body {
            font-family: 'Karla', sans-serif;
        }


        .content-wrapper {
            border-radius: 15px;
            box-shadow: 0 5px 8px 0 rgba(0,0,0,0.2);
            transition: 0.3s;
            border-radius: 10px;
            min-height: 190px !important;
            background-color: transparent;
        }

        .radio-checkbox {
            font-size: large;
            letter-spacing: 1px;
        }


        .title-card {
            color: #FFFFFF;
            background-color: #F2AA4C;
            padding: 15px;
            border-radius: 10px 10px 0px 0px;
            min-width: inherit;
        }

        .blue-color {
            background-color: #1d84C6 !important;
        }

        .green-color {
            background-color: #1CB394 !important;
        }

        .btn:active {
            background-color: #ffe7ab;
            transform: translateY(4px);
        }

        .btn:hover {
            background-color: #ffe7ab;
        }

        .td, th {
            vertical-align: middle;
        }

        table {
            border-collapse: collapse;
            width: 100%;
        }

        th, td {
            padding: 8px;
            text-align: left;
            border-top: 1px solid #ddd;
            border-bottom: 1px ridge #ddd;
        }

            td .style8 {
                font-weight: bold;
            }
    </style>
</head>
<body>

    <div class="px-3 mt-3">
        <div class="content-wrapper p-3">
            <span style=" background-color:aliceblue; font-weight:bolder; margin-bottom: 20px;"><i class="fas fa-edit" style="margin-right: 20px;"></i> DETAIL PROPOSAL PENELITIAN</span>
            <hr />

            <nav>
                <div class="nav nav-tabs" id="nav-tab" role="tablist">
                    <a class="nav-item nav-link active" id="nav-data-tab" data-toggle="tab" href="#nav-data" role="tab" aria-controls="nav-data" aria-selected="true">Data Penelitian</a>
                    <a class="nav-item nav-link" id="nav-identitas-tab" data-toggle="tab" href="#nav-identitas" role="tab" aria-controls="nav-identitas" aria-selected="false">Identitas Peneliti</a>
                    <a class="nav-item nav-link" id="nav-cv-tab" data-toggle="tab" href="#nav-cv" role="tab" aria-controls="nav-cv" aria-selected="false">CV-Pengalaman Penelitian (5 Tahun terakhir)</a>
                    <a class="nav-item nav-link" id="nav-publikasi-tab" data-toggle="tab" href="#nav-publikasi" role="tab" aria-controls="nav-publikasi" aria-selected="false">Publikasi</a>

                </div>
            </nav>

            <div class="tab-content" id="nav-tabContent">
                <div class="tab-pane fade show active" id="nav-data" role="tabpanel" aria-labelledby="nav-data-tab">
                    <div class="content-wrapper p-3" id="content" style="margin-top:10px">
                        <form method="post" asp-action="UbahPenelitian" asp-controller="Profile" enctype="multipart/form-data">

                            @Html.AntiForgeryToken()

                            <div class="row">
                                <div class="col-md-6">
                                    <label for="basic-addon1" class="form-label">ID PROPOSAL</label>
                                    <input type="text" class="form-control" id="id_proposal" name="id_proposal" value="@Model.refpropo.ID_PROPOSAL" required>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Skim Penelitian</label>
                                        <div class="input-group">
                                            <select style="width:710px">
                                                <option hidden style="color:lightgrey">Pilih Skim Penelitian</option>
                                                @foreach (var item in Model.refjenis1)
                                                {
                                                    <option value="@item.ID_SKIM"> @item.DESKRIPSI</option>

                                                }
                                            </select>
                                        </div>
                                    </div>

                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Tahun Akademik</label>
                                        <div class="input-group">
                                            <select style="width:710px">
                                                <option hidden style="color:lightgrey">Pilih Tahun Akademik</option>
                                                @foreach (var item in Model.refjenis2)
                                                {
                                                    <option value="@item.ID_TAHUN_AKADEMIK"> @item.TAHUN_AKADEMIK</option>

                                                }
                                            </select>
                                        </div>
                                    </div>

                                    <div class="mb-4">
                                        <label for="basic-addon2" class="form-label">Semester Akademik </label>
                                        <div class="input-group">
                                            <select style="width:710px">
                                                <option hidden style="color:lightgrey">Pilih Semester Akademik</option>
                                                @foreach (var item in Model.refjenis3)
                                                {
                                                    <option value="@item.NO_SEMESTER"> @item.SEMESTER_AKADEMIK</option>

                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="mb-4 ">
                                        <label for="basic-addon1" class="form-label">Output</label>

                                        <div class="radio-checkbox">

                                            @foreach (var item in Model.refjenis4)
                                            {
                                                <input type="checkbox" value="@item.ID_OUTPUT"> @item.DESKRIPSI

                                            }
                                        </div>

                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Outcome</label>
                                        <div class="input-group">
                                            <select style="width:710px">
                                                <option hidden style="color:lightgrey">Pilih Outcome </option>
                                                @foreach (var item in Model.refOutcome)
                                                {
                                                    <option value="@item.ID_OUTCOME"> @item.DESKRIPSI</option>

                                                }
                                            </select>

                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Jarak dari Kampus UAJY</label>
                                        <div class="input-group">
                                            <span class="input-group-text" id="basic-addon1"><i class="mdi mdi-counter small" aria-hidden="true"> </i></span>
                                            <input type="text" class="form-control" value="@Model.dataPenelitian.JARAK_KAMPUS_KM" placeholder="Jarak dari Kampus UAJY" aria-label="Nomor Barcode" aria-describedby="basic-addon1" id="noBarcode">
                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Tanggal Mulai Penelitian</label>
                                        <div class="input-group">
                                            <span class="input-group-text" id="basic-addon1"><i class="mdi mdi-calendar small" aria-hidden="true"> </i></span>
                                            <input type="date" class="form-control" placeholder="DD/MM/YYYY">

                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Beban SKS</label>
                                        <div class="input-group">
                                            <span class="input-group-text" id="basic-addon1"><i class="mdi mdi-counter small" aria-hidden="true"> </i></span>
                                            <input type="text" class="form-control" value="@Model.dataPenelitian.BEBAN_SKS" placeholder="Beban SKS" aria-label="Nomor Barcode" aria-describedby="basic-addon1">
                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Dokumen Proposal/blind Proposal</label>
                                        <div class="input-group">
                                            <span class="input-group-text" id="basic-addon1"><i class="mdi mdi-counter small" aria-hidden="true"> </i></span>
                                            <input type="FILE" class="form-control" placeholder="Dokumen Proposal" aria-describedby="basic-addon1" >
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-6">
                                    <div class="mb-4">
                                        <label for="basic-addon3" class="form-label">Judul Penelitian</label>
                                        <div class="input-group">
                                            <input id="subject" name="subject" type="text" placeholder="  Judul Penelitian" value="@Model.dataPenelitian.JUDUL" style=" height:20px;width: 710px;">
                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Kata Kunci(Inggris)</label>
                                        <div class="input-group">
                                            <input type="text" value="@Model.dataPenelitian.KEYWORD" class="form-control" placeholder="Kata Kunci" aria-label="Masa Berlaku" aria-describedby="basic-addon1" id="berlakuAnggota">
                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Kategori Penelitian</label>
                                        <div class="input-group">
                                            <select style="width:710px">
                                                <option hidden style="color:lightgrey">Pilih Kategori Penelitian</option>
                                                @foreach (var item in Model.refjenis7)
                                                {
                                                    <option value="@item.ID_KATEGORI"> @item.DESKRIPSI</option>

                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Tema Penelitian Universitas</label>
                                        <div class="input-group">
                                            <select style="width:710px">
                                                <option hidden style="color:lightgrey">Pilih Tema Penelitian</option>
                                                @foreach (var item in Model.refjenis6)
                                                {
                                                    <option value="@item.ID_TEMA_UNIVERSITAS"> @item.DESKRIPSI</option>

                                                }
                                            </select>
                                        </div>
                                    </div>

                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Jenis Penelitian</label>
                                        <div class="input-group">
                                            <select style="width:710px">
                                                <option hidden style="color:lightgrey">Pilih Jenis Penelitian</option>
                                                @foreach (var item in Model.refjenis5)
                                                {
                                                    <option value="@item.ID_JENIS_PENELITIAN"> @item.JENIS_PENELITIAN</option>

                                                }
                                            </select>

                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Lokasi Penelitian</label>
                                        <div class="input-group">
                                            <input id="pac-input"
                                                   class="controls"
                                                   type="text"
                                                   placeholder="Search Box">
                                            <div id="map"></div>
                                            <script src="https://maps.googleapis.com/maps/api/js?AIzaSyAOPreDHEYqILNxTGIiSuaYMHSnLzVFlVc&callback=initAutocomplete&libraries=places&v=weekly" async="" defer="defer" type="text/javascript"></script>


                                        </div>
                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Peta Lokasi</label>


                                    </div>
                                    <div class="mb-4">
                                        <label for="basic-addon1" class="form-label">Dokumen Lembar Pengesahan</label>
                                        <div class="input-group">
                                            <input type="file" class="form-control" placeholder="Dokumen Lembar Pengesahan" aria-describedby="basic-addon1">
                                        </div>
                                    </div>
                                    <hr />
                                    <div class="row">
                                        <div class="col-3">
                                            <button type="submit" class="btn btn-success">Simpan</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="tab-pane fade" id="nav-identitas" role="tabpanel" aria-labelledby="nav-identitas-tab">
                    <br />

                    <div class="content-wrapper p-3" id="content">

                        <span style="  font-weight:bolder; margin-bottom: 20px;"><i class="fas fa-user" style="margin-right: 5px;"></i> KETUA PENELITI</span>
                        <br>
                        <div style="width: 100%; overflow-x: auto;" class="col">
                            <table>
                                <thead>
                                    <tr>
                                        <td class="style8">
                                            NPP
                                        </td>
                                        <td>
                                            @Model.data.NPP
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="style8">
                                            Nama
                                        </td>
                                        <td>
                                            @Model.data.NAMA_LENGKAP_GELAR
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="style8">
                                            Jabatan Fungsional
                                        </td>
                                        <td>
                                            @Model.data2.DESKRIPSI
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="style8">
                                            Pangkat/Golongan
                                        </td>
                                        <td>
                                            @Model.data6.DESKRIPSI/@Model.data3.ID_REF_GOLONGAN
                                        </td>
                                    </tr>

                                    <tr>
                                        <td class="style8">
                                            NPP/NIDN
                                        </td>
                                        <td>

                                            @Model.data.NPP / @Model.data.NIDN
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="style8">
                                            Unit/Fakultas/Jurusan
                                        </td>
                                        <td>
                                            @Model.data4.NAMA_UNIT/@Model.data5.NAMA_UNIT
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="style8">
                                            Alamat
                                        </td>
                                        <td>
                                            @Model.data.ALAMAT
                                        </td>
                                    </tr>

                                    <tr>
                                        <td class="style8">
                                            No. Telp/Faks
                                        </td>
                                        <td>
                                            @Model.data.NO_TELPON_HP
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="style8">
                                            Email
                                        </td>
                                        <td>
                                            @Model.data.EMAIL_INSTITUSI
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="style8">
                                            Anggota Penelitian Kelompok
                                        </td>
                                        <td>
                                            <button style="margin-left: 2px; margin-top: 5px;" class="button4" onclick="getAnggota(@Model.refpropo.ID_PROPOSAL)"><i class="fas fa-eye"></i>&nbsp; Lihat Anggota</button>
                                        </td>
                                    </tr>


                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                        <button style="margin-left: 10px; margin-top: 40px;" class="button button1">Ubah Proposal</button>

                    </div>


                </div>
                <div class="tab-pane fade" id="nav-cv" role="tabpanel" aria-labelledby="nav-cv-tab">
                    <br>
                    <div class="content-wrapper p-3" id="content">
                        <div style="width: 100%; overflow-x: auto; margin-top: 10px;" class="col">
                            <table>
                                <thead>
                                    <tr class="text-center">
                                        <th>Tahun </th>
                                        <th>Judul</th>
                                        <th>Dana Pribadi </th>
                                        <th>Dana Eksternal </th>
                                        <th>Dana Kerjasama </th>
                                        <th>Dana UAJY</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            @Model.refpropo.TAHUN_AKADEMIK
                                        </td>
                                        <td> @Model.refpropo.JUDUL</td>
                                        <td>
                                            @Model.refpropo.DANA_PRIBADI
                                        </td>

                                        <td>      @Model.refpropo.DANA_EKSTERNAL </td>
                                        <td>
                                            @Model.refpropo.DANA_KERJASAMA
                                        </td>
                                        <td>@Model.refpropo.DANA_UAJY</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>

                        <button style="margin-left: 10px; margin-top: 40px;" class="button button1">Ubah Proposal</button>
                    </div>


                </div>

                <div class="tab-pane fade" id="nav-publikasi" role="tabpanel" aria-labelledby="nav-publikasi-tab">
                    <br>
                    <div class="content-wrapper p-3" id="content">
                        <div style="width: 100%; overflow-x: auto; margin-top: 10px" class="col">
                            <table>
                                <thead class="text-center">
                                    <tr>
                                        <th>Judul</th>
                                        <th> Aksi</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td> @Model.refpropo.JUDUL</td>
                                        <td class="text-center">
                                            <a class="btn btn-success" href="AddProsidingProfile?id_proposal=@Model.refpropo.ID_PROPOSAL" role="button" style="font-weight:bold">Prosiding</a>
                                            <a class="btn btn-warning" href="AddJurnalProfile?id_proposal=@Model.refpropo2.ID_PROPOSAL" role="button" style="font-weight:bold">Jurnal</a>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</body>

</html>

<!-- Modal Dialog 1 -->
<div class="modal fade" id="TambahAnggotaNonUAJY" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true" style="overflow-y:auto">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background-color:antiquewhite">
                <h5 class="modal-title" style="color:black; font-family:'Trebuchet MS';font-display:swap; transition:cubic-bezier()"> <i class="fas fa-clipboard"></i>&nbsp; Tambah Anggota Non UAJY</h5>

                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <br />
            <form class="formTambahAnggotaNonUAJY " method="post" asp-action="addAnggPenelitianNonUAJY" asp-controller="Profile" enctype="multipart/form-data" novalidate>
                @Html.AntiForgeryToken()

                <div class="modal-body">

                    <div class="mb-4">
                        <div class="form-group row" hidden>
                            <label class="col-sm-3 col-form-label">ID_PROPOSAL</label>
                            <input id="id_proposal" name="id_proposal" type="hidden" class="style1" style=" word-wrap:break-word" value="@Model.refpropo.ID_PROPOSAL" />

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label" >NPP </label>
                            <input id="npp" name="npp" type="text" class="style1" style=" word-wrap:break-word" placeholder="Masukan NPP"/>

                        </div>

                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                NIDN

                            </label>
                            <input type="text" id="nidn" name="nidn" class="style1"placeholder="Masukan NIDN" />

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                NPWP

                            </label>
                            <input type="text" id="npwp" name="npwp" class="style1" placeholder="Masukan NPWP"/>

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                Nama

                            </label>
                            <input type="text" id="nama_lengkap_gelar" name="nama_lengkap_gelar" class="style1"placeholder="Masukan Nama" />

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                Jabatan Fungsional

                            </label>
                            <input type="text" id="jabatanFungsional" name="jabatanFungsional" class="style1"placeholder="Masukan Jabatan Fungsional" />

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                Pangkat, Golongan

                            </label>
                            <input type="text" id="pangkat" name="pangkat" class="style1 col-md-3 " style="margin-right:30px" placeholder="Masukan Pangkat"/>
                            <input type="text" id="golongan" name="golongan" class="style1 col-md-3" placeholder="Masukan Golongan"/>

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                Bidang Keahlian

                            </label>
                            <input type="text" id="bidangKeahlian" name="bidangKeahlian" class="style1"placeholder="Masukan Bidang Keahlian" />

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                Unit/Fakultas
                            </label>
                            <input type="text" id="fakultas" name="fakultas" class="style1" placeholder="Masukan Unit/Fakultas" />

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                Jurusan/Prodi

                            </label>
                            <input type="text" id="prodi" name="prodi" class="style1" placeholder="Masukan Jurusan/Prodi"/>

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                Alamat
                            </label>
                            <input type="text" id="alamat" name="alamat" class="style1" placeholder="Masukan Alamat"/>

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                Kota

                            </label>
                            <input type="text" id="alamat_kota" name="alamat_kota" class="style1" placeholder="Masukan Kota"/>

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                Provinsi

                            </label>
                            <input type="text" id="alamat_provinsi" name="alamat_provinsi" class="style1"placeholder="Masukan Provinsi" />

                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-form-label">
                                Kode Pos

