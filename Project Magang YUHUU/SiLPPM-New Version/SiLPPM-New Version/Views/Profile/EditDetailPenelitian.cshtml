<script src="https://cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
<script src="https://cdn.datatables.net/1.10.25/js/dataTables.bootstrap4.min.js"></script>
<script>
    var tempIdTambah;
    var tableanggota;
    var tableanggotaUAJY;

    refreshTableAnggota();
    refreshTableAnggotaUAJY();

    function refreshTableAnggota() {
        tableanggota = $("#tableanggota").DataTable({

            "paging": true,
            "lengthChange": true,
            "searching": true,
            "ordering": true,
            "info": true,
            "autoWidth": false,
            "responsive": true,

        });
    }
    mapboxgl.accessToken =
        "pk.eyJ1IjoicG9ydGFsdWFqeSIsImEiOiJja3R3ZmN5NGwwdTI5MnVteHhuNWdnOTkxIn0.f2R7OUqhkBaSbVxmC1pIUw";
    const coordinates = document.getElementById("coordinates");
    const map = new mapboxgl.Map({
        container: "map",
        style: "mapbox://styles/mapbox/streets-v11",
        center: [110.41402263910413, -7.780548008097142],
        zoom: 13,
    });

    //popup on UAJY marker
    const popup = new mapboxgl.Popup({ offset: 20, padding: 5 }).setHTML(
        "<strong>Universitas Atma Jaya Yogyakarta</strong>" +
        "<p>Jl. Babarsari No.44, Janti, Caturtunggal, Kec. Depok, Kabupaten Sleman, Daerah Istimewa Yogyakarta 55281</span>"
    );

    //create marker for UAJY
    const markerUAJY = new mapboxgl.Marker({
        color: "red",
    })
        .setLngLat([110.41402263910413, -7.780548008097142])
        .addTo(map)
        .setPopup(popup);

    //initialize geocoder
    const geocoder = new MapboxGeocoder({
        accessToken: mapboxgl.accessToken,
        marker: false,
        mapboxgl: mapboxgl,
    });

    map.addControl(geocoder);

    //create marker for destinantion
    const markerPoint = new mapboxgl.Marker({
        draggable: true,
    });

    //response when user interact with mapbox
    geocoder.on("result", (e) => {
        markerPoint.setLngLat(e.result.center).addTo(map);

        const lngLat = markerPoint.getLngLat();

        var to = [lngLat.lng, lngLat.lat];
        var from = [markerUAJY.getLngLat().lng, markerUAJY.getLngLat().lat];

        var distance = turf.distance(to, from, "kilometers").toPrecision(4);

        coordinates.style.display = "block";
        coordinates.innerHTML = `<strong>Information Marker</strong> </br>Longitude: ${lngLat.lng} <br/>Latitude : ${lngLat.lat} 
            <br/>Distance : ${distance} km`;
        document.getElementById("LONGITUDE").value = lngLat.lng;
        document.getElementById("LATITUDE").value = lngLat.lat;
        document.getElementById("jarak").value = distance;

        markerPoint.on("dragend", (e) => {
            const lngLat = markerPoint.getLngLat();

            var to = [lngLat.lng, lngLat.lat];
            var from = [markerUAJY.getLngLat().lng, markerUAJY.getLngLat().lat];

            var distance = turf.distance(to, from, "kilometers").toPrecision(4);

            coordinates.style.display = "block";
            coordinates.innerHTML = `<strong>Information Marker</strong> </br>Longitude: ${lngLat.lng} <br/>Latitude: ${lngLat.lat}
                <br/>Distance: ${distance} km`;
            document.getElementById("LONGITUDE").value = lngLat.lng;
            document.getElementById("LATITUDE").value = lngLat.lat;
            document.getElementById("jarak").value = distance;
        });
    });

    function refreshTableAnggotaUAJY() {
        tableanggotaUAJY = $("#tableanggotaUAJY").DataTable({

            "paging": true,
            "lengthChange": true,
            "searching": true,
            "ordering": true,
            "info": true,
            "autoWidth": false,
            "responsive": true,

        });
    }

    function addAnggotaNonUAJY(id_proposal) {
        id_proposal = id_proposal;
        this.tempIdProposal = id_proposal;
        $('#ShowAnggota').modal('hide');
        $('#TambahAnggotaNonUAJY').modal('show');
        $('#btnAddAnggota').html('Tambah');
        $('#id_proposal').val(id_proposal);
    }
    function addAnggotaUAJY(id_proposal) {
