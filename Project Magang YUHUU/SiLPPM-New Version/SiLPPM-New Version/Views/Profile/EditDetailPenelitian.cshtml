    function pushOutput() {
        var arrItem = [];

        $("#outputList div input[type=checkbox]").each(function (index, val) {
            var checkId = $(val).attr("Id");
            var value = $(val).attr("Value");
            var Ischecked = $("#" + checkId).is(":checked", true);

            if (Ischecked) {
                arrItem.push(value);
                document.getElementById("output").value = arrItem;
            }

            if (arrItem.length == 0) {
                document.getElementById("output").value = "";
            }
        })
    }

