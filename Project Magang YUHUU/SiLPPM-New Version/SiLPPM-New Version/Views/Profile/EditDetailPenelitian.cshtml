<script>
    var tempIdTambah;
    var tableanggota;



    function addAnggotaNonUAJY(id_proposal) {

        id_proposal = id_proposal;
        this.tempIdProposal = id_proposal;
        $('#ShowAnggota').modal('hide');
        $('#TambahAnggotaNonUAJY').modal('show');

        $('#btnAddAnggota').html('Tambah');
        $('#id_proposal').val(id_proposal);
    }
    function addAnggotaUAJY(id_proposal) {

        id_proposal = id_proposal;
        this.tempIdProposal = id_proposal;
        $('#ShowAnggota').modal('hide');
        $('#modalSearch').modal('show');
        $('#id_proposal').val(id_proposal);
        

    }
    function getAnggota(id_proposal) {

        id_proposal = id_proposal;
        this.tempIdProposal = id_proposal;

        $('#id_proposal').val(id_proposal);
        $.ajax({
            type: "GET",
            url: "/Profile/ajaxGetPeserta",
            cache: false,
            data: { id_proposal: id_proposal },
            dataType: "json",
            success: function (result) {
                $('#ShowAnggota').modal('show');
                var str = '';
                var data = result.data;
                for (var i = 0; i < data.length; i++) {
                    if (data[i].NAMA_LENGKAP_GELAR != "" && data[i].NAMA_LENGKAP_GELAR != null) {
                        str += '<tr><td>' + data[i].ID_PERSONIL_PENELITIAN + '</td><td>' + data[i].NPP + '</td><td>' + data[i].NAMA_LENGKAP_GELAR + '</td><td>' +
                            data[i].INSTITUSI_ASAL + '</td><td>' + data[i].BIDANG_KEAHLIAN_PENELITIAN + '</td><td>' + data[i].EMAIL + '</td><td>'
                            + data[i].NO_TELPON_RUMAH + '</td><td>' + data[i].NO_TELPON_HP + '</td><td>' + data[i].ALAMAT + '</td></tr>';
                    }
                }

                $(".bodyanggota").html(str);

            }
        })
    }

    $('#tableanggota').DataTable({
        "paging": false,
        "lengthChange": true,
        "searching": false,
        "ordering": true,
        "info": true,
        "autoWidth": false,
        "responsive": true,
    });

    $('#tableAnggotaUAJY').DataTable({
        "paging": true,
        "lengthChange": true,
        "searching": true,
        "ordering": true,
        "info": true,
        "autoWidth": false,
        "responsive": true,
    });


    function myFunction() {
        var input, filter, table, tr, td, i, txtValue;
        input = document.getElementById("myInput");
        filter = input.value.toUpperCase();
        table = document.getElementById("myTable");
        tr = table.getElementsByTagName("tr");
        for (i = 0; i < tr.length; i++) {
            td = tr[i].getElementsByTagName("td")[0];
            if (td) {
                txtValue = td.textContent || td.innerText;
                if (txtValue.toUpperCase().indexOf(filter) > -1) {
                    tr[i].style.display = "";
                } else {
                    tr[i].style.display = "none";
                }
            }
        }
    }



    window.addEventListener('load', function () {
        /*var forms = document.querySelectorAll('.needs-validation')*/
        var forms = document.getElementsByClassName('formTambahAnggotaNonUAJY');
        var validation = Array.prototype.filter.call(forms, function (form) {
            form.addEventListener('submit', function (event) {
                if (!form.checkValidity()) {
                    event.preventDefault();
                    event.stopPropagation();
                }
                form.classList.add('was-validated');
            }, false);
        });
    }, false);

    function addAnggotaPenelitianUAJY( id_proposal, nppAnggota) {
        //var self = this;
        console.log(nppAnggota);
        console.log(id_proposal);
        var nppAnggota = this.value;
        var id_proposal = this.value;
        // masih ngebug diisinii
        
        /*var myTable = $('#tableanggotaUAJY').DataTable();*/

        $.ajax({
            type: "POST",
            url: "/Profile/AddAnggotaUAJY/" + id_proposal + "/" + nppAnggota,
            data: { id_proposal : id_proposal , nppAnggota : nppAnggota },
            success: function (data) {
                
                Swal.fire({
                    icon: 'success',
                    title: 'Data berhasil diTAMBAH!',
                    confirmButtonText: 'Tutup',
                })
                closeModal();
                window.location.reload();

            },
            error: function () {
                Swal.fire({
                    icon: 'error',
                    title: 'SiLPPM said: ',
                    text: 'Data gagal diTAMBAH!',
                })
                window.location.reload();
            }
        });
    }

</script>