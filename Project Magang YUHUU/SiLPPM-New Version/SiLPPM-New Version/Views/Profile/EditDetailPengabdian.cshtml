    <script src="https://cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.25/js/dataTables.bootstrap4.min.js"></script>

    @section Scripts{

        <script>
            var tblDataDaftarPengabdian;
            refreshTabelDaftarPengabdian();


            //Onload handler to fire off the app.
            //google.maps.event.addDomListener(window, 'load', initialize);
            mapboxgl.accessToken = 'pk.eyJ1IjoicG9ydGFsdWFqeSIsImEiOiJja3R3ZmN5NGwwdTI5MnVteHhuNWdnOTkxIn0.f2R7OUqhkBaSbVxmC1pIUw';
            const coordinates = document.getElementById('coordinates');
            const map = new mapboxgl.Map({
                container: 'map',
                style: 'mapbox://styles/mapbox/streets-v11',
                center: [110.41402263910413, -7.780548008097142],
                zoom: 13
            });

            //popup on UAJY marker
            const popup = new mapboxgl.Popup({ offset: 20, padding: 5 })
                .setHTML('<strong>Universitas Atma Jaya Yogyakarta</strong>' +
                    '<p>Jl. Babarsari No.44, Janti, Caturtunggal, Kec. Depok, Kabupaten Sleman, Daerah Istimewa Yogyakarta 55281</span>');

            //create marker for UAJY
            const markerUAJY = new mapboxgl.Marker(
                {
                    color: 'red'
                })
                .setLngLat(
                    [110.41402263910413, -7.780548008097142])
                .addTo(map)
                .setPopup(popup);

            //initialize geocoder
            const geocoder = new MapboxGeocoder({
                accessToken: mapboxgl.accessToken,
                marker: false,
                mapboxgl: mapboxgl,
            });

            map.addControl(geocoder);

            //create marker for destinantion
            const markerPoint = new mapboxgl.Marker({
                draggable: true
            });

            //response when user interact with mapbox
            geocoder.on('result', e => {
                markerPoint.setLngLat(e.result.center).addTo(map);

                const lngLat = markerPoint.getLngLat();

                var to = [lngLat.lng, lngLat.lat];
                var from = [markerUAJY.getLngLat().lng, markerUAJY.getLngLat().lat];

                var distance = (turf.distance(to, from, 'kilometers')).toPrecision(4);

                coordinates.style.display = 'block';
                coordinates.innerHTML = `<strong>Information Marker</strong> </br>Longitude: ${lngLat.lng} <br/>Latitude : ${lngLat.lat} <br/>Distance : ${distance} km`;
                document.getElementById("longitude").value = lngLat.lng;
                document.getElementById("latitude").value = lngLat.lat;
                document.getElementById("jarak").value = distance;

                markerPoint.on('dragend', e => {
                    const lngLat = markerPoint.getLngLat();

                    var to = [lngLat.lng, lngLat.lat];
                    var from = [markerUAJY.getLngLat().lng, markerUAJY.getLngLat().lat];

                    var distance = (turf.distance(to, from, 'kilometers')).toPrecision(4);

                    coordinates.style.display = 'block';
                    coordinates.innerHTML = `<strong>Information Marker</strong> </br>Longitude: ${lngLat.lng} <br/>Latitude: ${lngLat.lat} <br/>Distance: ${distance} km`;
                    document.getElementById("longitude").value = lngLat.lng;
                    document.getElementById("latitude").value = lngLat.lat;
                    document.getElementById("jarak").value = distance;
                })
            });
            //untuk datapicker
            $(document).ready(function () {
                $(".datepicker").datepicker({
                    format: 'yyyy-mm-dd',
                    autoclose: true,
                    todayHighlight: true,
                });
                $("#AWAL").on('changeDate', function (selected) {
                    var startDate = new Date(selected.date.valueOf());
                    $("#AKHIR").datepicker('setStartDate', startDate);
                    if ($("#AWAL").val() > $("#AKHIR").val()) {
                        $("#AKHIR").val($("#AWAL").val());
                    }
                });
            });
            function pushOutput() {
                var arrItem = [];

                $("#outputList div input[type=checkbox]").each(function (index, val) {
                    var checkId = $(val).attr("Id");
                    var value = $(val).attr("Value");
                    var Ischecked = $("#" + checkId).is(":checked", true);

                    if (Ischecked) {
                        arrItem.push(value);
                        document.getElementById("output").value = arrItem;
                    }

                    if (arrItem.length == 0) {
                        document.getElementById("output").value = "";
                    }
                })
            }

            function pushOutcome() {
                var arrItem = [];

                $("#outcomeList div input[type=checkbox]").each(function (index, val) {
                    var checkId = $(val).attr("Id");
                    var value = $(val).attr("Value");
                    var Ischecked = $("#" + checkId).is(":checked", true);
                    if (Ischecked) {
                        arrItem.push(value);
                        document.getElementById("outcome").value = arrItem;

                    }

                    if (arrItem.length == 0) {
                        document.getElementById("outcome").value = "";
                    }
                })
            }

            function refreshTabelDaftarPengabdian() {
                tblDataDaftarPengabdian = $("#tblDataHistoryPenelitian").DataTable({

                    "paging": true,
                    "lengthChange": true,
                    "searching": true,
                    "ordering": true,
                    "info": true,
                    "autoWidth": false,
                    "responsive": true,
                }).buttons().container().appendTo('#tblDataHistoryPenelitian_wrapper .col-md-6:eq(0)');
            }


        </script>
    }
</body>
</html>
